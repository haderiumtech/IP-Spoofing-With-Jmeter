            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Get the HTTP Response Code
int responseCode = prev.getResponseCode().toInteger();

// Log the response code (for debugging)
log.info(&quot;Sign-Up Response Code: &quot; + responseCode);

// Check if the response code is 200
if (responseCode == 200) {
    vars.put(&quot;signupSuccess&quot;, &quot;true&quot;);
} else {
    vars.put(&quot;signupSuccess&quot;, &quot;false&quot;);
}
</stringProp>
